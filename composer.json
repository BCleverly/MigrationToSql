{
    "name": "cleverly/migrationtosql",
    "description": "This is my package MigrationToSql",
    "keywords": [
        "Cleverly",
        "laravel",
        "migrationtosql"
    ],
    "homepage": "https://github.com/BCleverly/MigrationToSql",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben",
            "email": "ben@bencleverly.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "doctrine/sql-formatter": "^1.1",
        "illuminate/contracts": "^8.53",
        "illuminate/database": "^8.53",
        "spatie/laravel-package-tools": "^1.9.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.3",
        "nunomaduro/collision": "^5.6",
        "orchestra/testbench": "^6.19",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.9.2"
    },
    "autoload": {
        "psr-4": {
            "Cleverly\\MigrationToSql\\": "src",
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cleverly\\MigrationToSql\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cleverly\\MigrationToSql\\MigrationToSqlServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
